{
    "name": "digitalunityca/backpack-translatable",
    "description": "Translatable for backpack for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Oleg Hmelevschi",
            "email": "info@digitalunity.ca"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "backpack/base": "*",
        "backpack/crud": "*",
        "backpack/langfilemanager": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalUnityCa\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalUnityCa\\Translatable\\Test\\": "tests"
        }
    },
    "require-dev": {
        "php": "^7.1.3",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalUnityCa\\Translatable\\TranslatableServiceProvider"
            ],
            "aliases": {
                "Translatable": "DigitalUnityCa\\Translatable\\TranslatableFacade"
            }
        }
    },
    "scripts": {
        "post-package-install": [

        ]
    }

}
